<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.myapp.mapper.UserMapper">

    <insert id="insert" parameterType="com.example.myapp.domain.User" useGeneratedKeys="true" keyProperty="memNo">
        INSERT INTO tb_member (name, password, roles) VALUES (#{name}, #{password}, #{roles})
    </insert>


</mapper>
